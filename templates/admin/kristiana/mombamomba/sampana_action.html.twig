<div class="modal-header">
  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
      <span aria-hidden="true">Ã—</span>
  </button>
  <h4 class="modal-title">Hanampy sampana</h4>
</div>
{% if editMode %}
{{form_start(form,{'attr':{
  'id':"sampanaKristianaForm",
  'class': sampana.id
}})}}
{% else %}
{{form_start(form,{'attr':{
  'id':"sampanaKristianaForm",
  'class': ''
}})}}
{% endif %}
<div class="modal-body">
  <div class="form-group">
    <label class="form-control-label" for="inputBasicEmail">Sampana/Fikambanana *</label>
    {{form_widget(form.sampana,{'attr':{
      'class': "form-control",
      'placeholder': 'Sokajy',
      'autocomplete': "off",
      'data-plugin':"selectpicker"
    }})}}
  </div>
  <div class="form-group">
    <label class="form-control-label" for="inputBasicEmail">Andraikitra *</label>
    {{form_widget(form.toerana,{'attr':{
      'class': "form-control",
      'placeholder': 'Andraikitra',
      'autocomplete': "off",
      'data-plugin':"selectpicker"
    }})}}
  </div>
</div>
<div class="modal-footer">
  <button type="submit" class="btn btn-primary btn-block">Tehirizo</button>
  <button type="button" class="btn btn-default btn-block" data-dismiss="modal">Ajanona</button>
</div>
{{form_end(form)}}